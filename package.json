        "dotenv": "^16.0.3",
        "jwt-decode": "^3.1.2",
        "react": "^18.2.0",
        "react-chartjs-2": "^5.2.0",
        "react-dom": "^18.2.0",
        "react-hook-form": "^7.43.8",
        "react-particles": "^2.9.3",
        "react-phone-input-2": "^2.15.1",

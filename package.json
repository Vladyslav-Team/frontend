        "use-debounce": "^9.0.4"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "npx prettier --check . && eslint . --ext .js --ext .jsx",
        "lint-fix": "eslint . --ext .js --ext .jsx --fix",
        "format": "npx prettier --write ."
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run format && git diff --cached --name-only --diff-filter=ACMRT | grep '\\.js$' | xargs npm run lint-fix && git add .",
            "pre-push": "npm test && npm run build"
        }
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "axios-mock-adapter": "^1.21.2",
        "eslint": "^8.36.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-react": "^7.32.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "husky": "^4.3.8",
        "json-loader": "^0.5.7",
        "prettier": "2.8.6"
    }
